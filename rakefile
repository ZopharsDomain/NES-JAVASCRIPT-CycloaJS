require 'erb'

generated = ["build/cycloajs-src.js"];


task :default => generated;

file "build/cycloajs-src.js" => "js/cycloa/fast.js" do |t|
	sh "jake"
end

file "js/cycloa/fast.js" => FileList["src/fast*.erb.js", "src/*.rb"] do |t|
	puts "generating emulation core"
	sh "erb src/fast.erb.js > #{t.name}" 
end

task :clean do
	rm generated
end
	

